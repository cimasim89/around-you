{
  "version": 2,
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "defaultProject": "webapp",
  "generators": {
    "@nrwl/react": {
      "application": {
        "style": "css",
        "linter": "eslint",
        "strict": true,
        "babel": true
      },
      "component": {
        "style": "css"
      },
      "library": {
        "style": "css",
        "linter": "eslint",
        "strict": true
      }
    }
  },
  "projects": {
    "webapp": {
      "root": "apps/webapp",
      "sourceRoot": "apps/webapp/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/webapp",
            "index": "apps/webapp/src/index.html",
            "main": "apps/webapp/src/main.tsx",
            "polyfills": "apps/webapp/src/polyfills.ts",
            "tsConfig": "apps/webapp/tsconfig.app.json",
            "assets": [
              "apps/webapp/src/favicon.ico",
              "apps/webapp/src/assets"
            ],
            "styles": [
              "apps/webapp/src/styles.css"
            ],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/webapp/src/environments/environment.ts",
                  "with": "apps/webapp/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "webapp:build",
            "hmr": true,
            "proxyConfig": "apps/webapp/proxy.conf.json"
          },
          "configurations": {
            "production": {
              "buildTarget": "webapp:build:production",
              "hmr": false
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/webapp/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/webapp"
          ],
          "options": {
            "jestConfig": "apps/webapp/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "webapp-e2e": {
      "root": "apps/webapp-e2e",
      "sourceRoot": "apps/webapp-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/webapp-e2e/cypress.json",
            "tsConfig": "apps/webapp-e2e/tsconfig.e2e.json",
            "devServerTarget": "webapp:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "webapp:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/webapp-e2e/**/*.{js,ts}"
            ]
          }
        }
      }
    },
    "back-end-service": {
      "root": "apps/back-end-service",
      "sourceRoot": "apps/back-end-service/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/back-end-service",
            "main": "apps/back-end-service/src/main.ts",
            "tsConfig": "apps/back-end-service/tsconfig.app.json",
            "assets": [
              "apps/back-end-service/src/assets"
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/back-end-service/src/environments/environment.ts",
                  "with": "apps/back-end-service/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "back-end-service:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/back-end-service/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/back-end-service"
          ],
          "options": {
            "jestConfig": "apps/back-end-service/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  }
}